{
  "title": "zookeeper",
  "description": "zookeeper,分布式协调框架",
  "author": "Victor <victorfengming@163.com>",
  "language": "zh-hans",
  "gitbook": "3.2.3",
  "root": "./src",
  "links": {
    "sidebar": {
      "文档索引页": "https://victorfengming.gitee.io/index/gitbook/"
    }
  },
  "plugins": [
    "code",
    "valine",
    "hide-element",
    "chapter-fold",
    "expandable-chapters",
    "favicon",
    "lightbox",
    "splitter",
    "-lunr",
    "pageview-count",
    "ancre-navigation",
    "-search",
    "search-plus",
    "-highlight",
    "popup",
    "prism",
    "prism-themes",
    "-sharing",
    "sharing-plus",
    "tbfed-pagefooter",
    "toolbar-button"
  ],
  "pluginsConfig": {
    "toolbar-button": {
      "url": "https://victorfengming.gitee.io/zookeeper/",
      "icon": "fa-file-pdf-o",
      "label": "下载PDF"
    },
    "prism": {
      "css": [
        "prism-themes/themes/prism-atom-dark.css"
      ]
    },
    "valine": {
      "appId": "CHjATxRcQrXst8eJrdwX0vjz-gzGzoHsz",
      "appKey": "nWerbwV2WMAxOEmAMkJKvXzs"
    },
    "favicon": {
      "shortcut": "./img/avator.png.icon.ico",
      "bookmark": "./img/avator.png.icon.ico"
    },
    "hide-element": {
      "elements": [
        ".gitbook-link"
      ]
    },
    "sharing": {
      "douban": false,
      "facebook": false,
      "google": false,
      "hatenaBookmark": false,
      "instapaper": false,
      "line": false,
      "linkedin": false,
      "messenger": false,
      "pocket": false,
      "qq": true,
      "qzone": true,
      "stumbleupon": false,
      "twitter": true,
      "viber": false,
      "vk": false,
      "weibo": true,
      "whatsapp": false,
      "all": [
        "douban",
        "facebook",
        "google",
        "instapaper",
        "line",
        "linkedin",
        "messenger",
        "pocket",
        "qq",
        "qzone",
        "stumbleupon",
